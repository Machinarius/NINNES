[*.cs]
indent_style = tab
indent_size = 4
insert_final_newline = true
csharp_new_line_before_open_brace = all
csharp_new_line_before_else = all
csharp_new_line_before_catch = all
csharp_new_line_before_finally = all
csharp_new_line_before_members_in_object_initializers = all
csharp_new_line_before_members_in_anonymous_types = all
csharp_new_line_between_query_expression_clauses = all
dotnet_sort_system_directives_first = true
